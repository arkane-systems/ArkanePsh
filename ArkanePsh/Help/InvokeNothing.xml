<?xml version='1.0'?>

<Cmdlet FullName='ArkaneSystems.ArkanePsh.Commands.NoOp'>
  <Description>
    Does nothing.
  </Description>
  <DetailedDescription>
    Does nothing, either succeeding or failing.
  </DetailedDescription>
  <Parameters>
    <Parameter Name='InputObject'>
      <Description>
        Something to do nothing to.
      </Description>
      <DefaultValue>

      </DefaultValue>
    </Parameter>
    <Parameter Name='Fail'>
      <Description>
        Fail at doing nothing.
      </Description>
      <DefaultValue>

      </DefaultValue>
    </Parameter>
  </Parameters>

  <InputTypes>
    <InputType>
      <Name>System.Object</Name>
      <Description>
        <p>Any object.</p>
      </Description>
    </InputType>
  </InputTypes>

  <ReturnTypes>
    <ReturnType>
      <Name>Nothing, or System.Object.</Name>
      <Description>
        <p>The input object, unchanged.</p>
      </Description>
    </ReturnType>
  </ReturnTypes>

  <Examples>
    <Example Number="1">
      <Code>Invoke-Nothing</Code>
      <Remarks>
        <p>Do nothing and succeed.</p>
      </Remarks>
    </Example>
    <Example Number="2">
      <Code>Invoke-Nothing -Fail</Code>
      <Remarks>
        <p>Do nothing and fail.</p>
      </Remarks>
    </Example>
  </Examples>

  <Notes>
    <Note>
      <p></p>
    </Note>
  </Notes>

</Cmdlet>